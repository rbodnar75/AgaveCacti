<aruba_oscx_6x00_fan>
	<name>Aruba OSCX 6x00 switch - VSF members</name>
	<script_path>|path_cacti|/scripts/ss_aruba_oscx_6x00.php</script_path>
	<script_function>ss_aruba_oscx_6x00</script_function>
	<script_server>php</script_server>
	<arg_prepend>|host_id| vsf</arg_prepend>
	<arg_index>index</arg_index>
	<arg_num_indexes>num_index</arg_num_indexes>
	<arg_query>query</arg_query>
	<arg_get>get</arg_get>
	<output_delimiter>:</output_delimiter>
	<index_order>vsfIndex</index_order>
	<index_order_type>numeric</index_order_type>
	<index_title_format>Check#|chosen_order_field|</index_title_format>

	<fields>
		<vsfIndex>
			<name>VSF member/standalone index</name>
			<direction>input</direction>
			<query_name>index</query_name>
		</vsfIndex>
		<vsfName>
			<name>Name or serial</name>
			<direction>input-output</direction>
			<query_name>name</query_name>
		</vsfName>
		<vsfState>
			<name>VSF member state (3=ok)</name>
			<direction>input-output</direction>
			<query_name>state</query_name>
		</vsfState>
		<vsfCPU>
			<name>VSF member CPU</name>
			<direction>output</direction>
			<query_name>cpu</query_name>
		</vsfCPU>
		<vsfMemory>
			<name>VSF member memory</name>
			<direction>output</direction>
			<query_name>memory</query_name>
		</vsfMemory>
		<vsfUptime>
			<name>VSF member uptime</name>
			<direction>output</direction>
			<query_name>uptime</query_name>
		</vsfUptime>

	</fields>
</aruba_oscx_6x00_fan>
